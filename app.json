{
  "expo": {
    "name": "Tienda Bomberos",
    "slug": "tienda-bomberos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/elLogo-1.png",
    "scheme": "tienda-bomberos",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/elLogo-1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tiendabomberos.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.tiendabomberos.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/elLogo-1.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK",
        "android.permission.INTERNET",
        "com.google.android.c2dm.permission.RECEIVE"
      ],
      "usesCleartextTraffic": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/elLogo-1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Esta app necesita acceso a la ubicaci√≥n para encontrar direcciones cercanas."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "networkSecurityConfig": {
              "cleartextTrafficPermitted": true,
              "domainConfig": [
                {
                  "domains": [
                    "181.49.225.64"
                  ],
                  "cleartextTrafficPermitted": true
                }
              ]
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "EXPO_PUBLIC_API_URL": "http://181.49.225.64:3001/api/v1",
      "EXPO_PUBLIC_API_BASE_URL": "http://181.49.225.64:3001",
      "EXPO_PUBLIC_STAGE": "production",
      "eas": {
        "projectId": "bda43edf-60e4-4418-875a-0e057ac9c3a3"
      }
    },
    "owner": "angeldacid"
  }
}
